#include <stdio.h>

int main(void)
{
    int a = 1;   // 변수 a를 선언하고 1로 초기화
    int i;       // 반복 횟수를 세기 위한 변수 i 선언

    for (i = 0; i < 3; i++) // i를 0부터 시작해서 3보다 작을 동안 반복 (총 3회 반복)
    {
        a = a * 2;          // a의 값을 2배로 갱신
    }

    printf("a : %d\n", a);  // 최종적으로 계산된 a 값을 출력
    return 0;
}
